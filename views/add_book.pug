extends layout 

block content
	h1 Add Book
	p This is where you add your new book.
	hr
	form(method='POST', action='/books/add')
		div
			label Title: 
			input(name='title', type='text')
		div 
			label Genre: 
			input(name='genre', type='text')
		div
			button.btn.btn-primary(type="submit") Create 